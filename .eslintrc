{
  "extends": "@rocketseat/eslint-config/node",
  "rules": {
    "@typescript-eslint/no-explicit-any": "off",
    "@typescript-eslint/no-unused-vars": "off",
    "camelcase": "off",
    "@typescript-eslint/no-var-requires": "off",
    "prettier/prettier": [
      "error",
      {
        "semi": false,
        "singleQuote": false,
        "endOfLine": "auto"
      }
    ]
  }
}